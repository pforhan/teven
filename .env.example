# Whether to pre-populate data
DEV_MODE=false

# PostgreSQL Database Settings
TEVEN_POSTGRES_DB=teven_db
TEVEN_POSTGRES_USER=teven_user
TEVEN_POSTGRES_PASSWORD=teven_password

# Backend Application Port
# The external port your machine will use to access the backend.
TEVEN_BACKEND_PORT=2022

# Super Admin User - used to seed the database with an initial admin account
SUPERADMIN_EMAIL=admin@teven.com
SUPERADMIN_PASSWORD=super_secret_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_ISSUER=teven-issuer
JWT_AUDIENCE=teven-audience
# logged in sessions are 24 hours
JWT_EXPIRATION_MILLIS=86400000